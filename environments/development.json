{
  "name": "development",
  "description": "Environment for development tools like Jenkins, Chef-Server etc.",
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "cookbook_versions": {
    "java": "= 1.29.0",
    "wlc-jenkins": "= 0.1.0"
  },
  "default_attributes": {
    "java": {
      "install_flavor": "oracle",
      "jdk_version": "8",
      "oracle": {
        "accept_oracle_download_terms": true
      }
    },
    "jenkins": {
      "master": {
        "install_method": "package",
        "jvm_options": null,
        "jenkins_args": null,
        "user": "jenkins",
        "group": "jenkins",
        "use_system_accounts": true,
        "host": "localhost",
        "listen_address": "0.0.0.0",
        "port": 8080,
        "home": "/var/lib/jenkins",
        "log_directory": "/var/log/jenkins"
      }
    },
  	"vagrant": {
  		"url": "https://dl.bintray.com/mitchellh/vagrant/vagrant_1.6.5_x86_64.rpm",
  		"checksum": "90730fd10cbd811969ec58f28818685f3074f8399852dfd1d4858d75c4224fdc"
  	}
  },
  "override_attributes": {
    
  }
}
